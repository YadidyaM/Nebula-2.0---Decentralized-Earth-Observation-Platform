# Nebula Protocol Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
VITE_WS_URL=ws://localhost:8000/ws

# Solana Configuration
VITE_SOLANA_RPC_URL=https://api.devnet.solana.com
VITE_SOLANA_NETWORK=devnet

# Mapbox Configuration (for satellite imagery)
VITE_MAPBOX_TOKEN=acquire-key_to_use

# Gemini AI Configuration (for voice commands and chat)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# External API Keys 
VITE_AMBEE_API_KEY=your_ambee_api_key_here
VITE_NASA_API_KEY=DEMO_KEY
VITE_NOAA_API_TOKEN=your_noaa_token_here

# Development Configuration
VITE_DEBUG=false
VITE_MOCK_DATA=false
